
<core:View 
    xmlns:m="sap.m" 
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified" 
    controllerName="sap.mlf.bar8.controller.InferenceResult">
    <m:Page title="{i18n>appTitle}" showHeader="true">
        <m:FlexBox
            id="BoxImage" 
            fitContainer="true"
            alignItems="Center" 
            justifyContent="Center">
            <m:Image
                id="uploadedImage"
                width="70%" 
                height="70%" 
                densityAware="false" >
            </m:Image>
        </m:FlexBox>
    </m:Page>
</core:View>