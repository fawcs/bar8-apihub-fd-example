{
    "_version": "1.3.0",
    "sap.app": {
        "_version": "1.3.0",
        "id": "${project.artifactId}",
        "type": "application",
        "resources": "resources.json",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0"
        },
        "ach": "XYZ",
        "dataSources": {
        }
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [
            "F9999"
        ],
        "archeType": "transactional"
    },
    "sap.ui": {
        "_version": "1.3.0",
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.2.0",
        "rootView": {
            "viewName": "sap.mlf.bar8.view.App",
            "type": "XML",
            "id": "main"
        },
        "dependencies": {
            "minUI5Version": "${sap.ui5.dist.version}",
            "libs": {
                "sap.collaboration": {
                    "minVersion": "1.36",
                    "lazy": true
                },
                "sap.m": {
                    "minVersion": "1.36.0"
                },
                "sap.ui.comp": { },
                "sap.ui.core": {
                    "minVersion": "1.36.0"
                },
                "sap.ui.layout": { },
                "sap.ui.table": { },
                "sap.ushell": {
                    "minVersion": "1.36.0"
                },
                "sap.uxap": { }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "config": {
            "fullWidth" : true,
            "sapFiori2Adaptation": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "sap.mlf.bar8.i18n.i18n"
                }
            },
            "authentication": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                },
                "preload": true
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "sap.mlf.bar8.view",
                "controlId": "idAppControl",
                "controlAggregation": "pages",
                "viewType": "XML",
                "async": true,
                "transition": "slide"
            },
            "routes": [
                {
                    "name": "FaceRecognition",
                    "pattern": "",
                    "target": "faceRecognition"
                },
                {
                    "name": "InferenceResult",
                    "pattern": "result",
                    "target": "inferenceResult"
                }
            ],
            "targets": {
                "faceRecognition": {
                    "viewName": "FaceRecognition",
                    "viewLevel": 0
                },
                "inferenceResult": {
                    "viewName": "InferenceResult",
                    "viewLevel": 1
                }
            },
            "headerWhiteList": [
                "APIKey"
            ]
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                },
                {
                    "uri": "css/image.css",
                    "id": ""
                }
            ]
        }
    }
}
